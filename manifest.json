{
  "manifest_version": 3,
  "name": "move2edge_of_page",
  "version": "1.0",
  "description": "Scroll to the edge of the page with Ctrl + Left/right Arrow",
  "background": {
      "service_worker": "background.js"
  },
  "permissions": [
      "activeTab",
      "scripting"
  ],
  "host_permissions": [
      "http://*/*",
      "https://*/*",
      "file://*/*"
  ],
  "action": {}
}
